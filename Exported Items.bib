@article{Ferrer-Gonzalez2021,
  title = {Resource Partitioning of Phytoplankton Metabolites That Support Bacterial Heterotrophy},
  author = {{Ferrer-Gonz{\'a}lez}, Frank Xavier and Widner, Brittany and Holderman, Nicole R. and Glushka, John and Edison, Arthur S. and Kujawinski, Elizabeth B. and Moran, Mary Ann},
