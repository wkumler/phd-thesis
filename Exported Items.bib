@article{Fong2008,
  title = {Nitrogen Fixation in an Anticyclonic Eddy in the Oligotrophic {{North Pacific Ocean}}},
  author = {Fong, Allison A and Karl, David M and Lukas, Roger and Letelier, Ricardo M and Zehr, Jonathan P and Church, Matthew J},
  year = {2008},
  month = jul,
  journal = {The ISME Journal},
  volume = {2},
