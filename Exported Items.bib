@article{Johnson2020,
  title = {Metabolite Composition of Sinking Particles Differs from Surface Suspended Particles across a Latitudinal Transect in the {{South Atlantic}}},
  author = {Johnson, Winifred M. and Longnecker, Krista and Kido Soule, Melissa C. and Arnold, William A. and Bhatia, Maya P. and Hallam, Steven J. and Van Mooy, Benjamin A. S. and Kujawinski, Elizabeth B.},
