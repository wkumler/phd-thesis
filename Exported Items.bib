@article{Omand2015,
  title = {Eddy-Driven Subduction Exports Particulate Organic Carbon from the Spring Bloom},
  author = {Omand, Melissa M. and D'Asaro, Eric A. and Lee, Craig M. and Perry, Mary Jane and Briggs, Nathan and Cetini{\'c}, Ivona and Mahadevan, Amala},
  year = {2015},
  month = apr,
  journal = {Science},
  volume = {348},
