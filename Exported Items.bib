@article{Sacks2022,
  title = {Quantification of Dissolved Metabolites in Environmental Samples through Cation-exchange Solid-phase Extraction Paired with Liquid Chromatography--Mass Spectrometry},
  author = {Sacks, Joshua S. and Heal, Katherine R. and Boysen, Angela K. and Carlson, Laura T. and Ingalls, Anitra E.},
  year = {2022},
