@article{Bochdansky2021,
  title = {Adenosine Triphosphate ( {{{\textsc{ATP}}}} ) as a Metric of Microbial Biomass in Aquatic Systems: New Simplified Protocols, Laboratory Validation, and a Reflection on Data from the Literature},
  shorttitle = {Adenosine Triphosphate (},
  author = {Bochdansky, Alexander B. and Stouffer, Alison N. and Washington, NyJaee N.},
