@article{Stanley2023,
  title = {Late-season Nitrogen Uptake across the Western Coastal {{Alaskan Arctic}}},
  author = {Stanley, Brianna C. and Sipler, Rachel E. and Roberts, Quinn N. and Spackeen, Jenna L. and Norton, E. Zane and Bronk, Deborah A.},
