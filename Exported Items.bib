  title = {Phytoplankton Exudates Provide Full Nutrition to a Subset of Accompanying Heterotrophic Bacteria via Carbon, Nitrogen and Phosphorus Allocation},
  author = {Eigemann, Falk and Rahav, Eyal and Grossart, Hans-Peter and Aharonovich, Dikla and Sher, Daniel and Vogts, Angela and Voss, Maren},
  year = {2022},
