  author = {Barone, Benedetto and Church, Matthew J. and Dugenne, Mathilde and Hawco, Nicholas J. and Jahn, Oliver and White, Angelicque E. and John, Seth G. and Follows, Michael J. and DeLong, Edward F. and Karl, David M.},
  year = {2022},
