@article{Resplandy2019,
  title = {Effects of {{Eddy}}-{{Driven Subduction}} on {{Ocean Biological Carbon Pump}}},
  author = {Resplandy, Laure and L{\'e}vy, Marina and McGillicuddy, Dennis J.},
